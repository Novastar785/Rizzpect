{
  "expo": {
    "name": "Rizzflows",
    "slug": "rizzflow",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "rizzflow",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    },
    "ios": {
      "buildNumber": "8",
      "supportsTablet": false,
      "bundleIdentifier": "com.rizzflows.app.v2",
      "userInterfaceStyle": "dark",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Rizzflows needs access to your photo library to analyze conversation screenshots and generate intelligent replies.",
        "NSCameraUsageDescription": "Rizzflows needs access to your camera to take photos of your conversations for analysis.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#121212"
      },
      "splash": {
        "image": "./assets/images/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#121212"
      },
      "versionCode": 3,
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.rizzflows.app.v2",
      "userInterfaceStyle": "dark",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-localization",
      [
        "expo-image-picker",
        {
          "photosPermission": "Rizzflows needs access to your photo library to analyze conversation screenshots and generate intelligent replies.",
          "cameraPermission": "Rizzflows needs access to your camera to take photos of your conversations for analysis."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "lintOptions": {
              "checkReleaseBuilds": false,
              "abortOnError": false
            }
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c512c365-7e0c-4eb2-b4a0-920f026d14ae"
      }
    }
  }
}